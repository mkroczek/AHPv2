<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="miapd.ahp.controllers.MoreExpertsController"
          prefHeight="400.0" prefWidth="600.0" alignment="CENTER"
          spacing="20">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Czy chcesz wypełnić preferencje następnego eksperta?"/>

    <HBox alignment="CENTER" spacing="20">
        <Button text="Nie" onAction="#no"/>
        <Button text="Tak" onAction="#yes"/>
    </HBox>

</VBox>
