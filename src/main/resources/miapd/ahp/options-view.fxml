<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import miapd.ahp.gui.OptionsWindow?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="miapd.ahp.controllers.OptionsController"
          prefHeight="400.0" prefWidth="600.0" spacing="20" alignment="CENTER">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Wybierz opcje"/>
    <OptionsWindow fx:id="optionsWindow" alignment="CENTER"/>
    <Button text = "Proceed" onAction="#proceed"/>
</VBox>
